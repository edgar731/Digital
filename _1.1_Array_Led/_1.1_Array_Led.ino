//****************************  INCLUDES   ***********************************************************


//********************************  VARIABLES  *******************************************************
int arrayLed [] ={5,6,7,8,9,10,11,12};    

int i;


//*******************************  SETUP   ***********************************************************
void setup() {                            
for (i=0; i<8; i++)
{  
pinMode(arrayLed [i],OUTPUT);             
}

 

}

//****************************    LOOP   *************************************************************
void loop() {                            
 delay (2000);                        
 for (i=0; i<8; i++) {
 digitalWrite (arrayLed[i], HIGH);           

                    }
 delay (2000);                           
 for (i=0; i<8; i++) {
 digitalWrite (arrayLed[i], LOW);         
                    }
            }
